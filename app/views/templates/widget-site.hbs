        <div class="login miCuenta" id="headerNotLogin" style="display:block">
          <ul>
            <li class="link">Ver video</li>
            <li class="link">
              <a href="#register-layer" id="registerLink" class="fancybox">Registrate</a>
            </li>
            <li class="cart-placeholder">
                <span class="spanDropMenu"><span class="cart-items-count reseteable" data-reset-text="0">0</span><span class="icon shopCarMin"></span></span>
            </li>
            <li class="btn btnTopNav" id="btn-login">
              <a href="#login-layer"  class="wb-login-btn" style="color: #ffffff;text-decoration: none;">Log in</a>
            </li>
          </ul>
        </div>
        <div class="miCuenta" id="" style="">
