            <li class="drop-down-holder" id="liDisplayCuenta">
              <span class="spanDropMenu link triggerMiCuenta">Mi Cuenta<span class="icon trigger"></span></span>
              <div class="dropMenu miCuentaDiv">
                <span class="icon triangle"></span>
                <div class="wrapper">
                  <div class="colum myProfile" id="headerProfile">
                  </div>
                  <div class="colum myAddress" id="headerAddress">
                  </div>
