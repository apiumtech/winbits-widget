<div class="shippingAddresses chk-step" id="wbi-shipping-addresses-view">
  <ul>
      {{#each addresses}}
      <li class="shippingItem wb-shipping-address {{#if main}}shippingSelected{{/if}}" id="shippingItem-{{id}}">
        <div class="shippingMenu">
          <a href="#" class="edit-address" id="editAddress-{{id}}">edit</a>
          <span class="icon closeButton delete-address" id="deleteAddress-{{id}}">cerrar</span>
        </div>
        <h3>DIRECCIÓN {{index}}</h3>
        <address>
          <span>{{getContactName}},</span>
          <span>{{street}} {{externalNumber}}, {{getLocation}},</span>
          <span>{{county}}, {{getZipCode}}</span>
        </address>
        <span class="note send direccionSend">Tu paquete será enviado a esta dirección</span>
        <input type="radio" name="winbitsShippingRadio" class="shippingRadio" id="winbitsShippingRadio1">
      </li>
      {{/each}}
    <li class="shippingAdd">
      <h3>AGREGAR DIRECCIÓN +</h3>
      <span id="aNewAddress">Da click para agregar</span>
    </li>
  </ul>
  <input type="button"  id="btnContinuar" class="btn btnCheckout" value="Continuar">
</div>
<div id="wbi-shipping-new-address-container" class="shippingNewAddress"></div>
<div id="wbi-edit-shipping-address-container" class="shippingNewAddress"></div>
